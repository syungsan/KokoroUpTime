            <Grid x:Name="Step3EvaluateAkamaruMethodGrid" Margin="130,80,130,180" d:IsHidden="True">
                    <TextBlock x:Name="EvaluateAkamaruMethodTitleText" Text="他の方法について、一番良い方法を探てみよう！" FontSize="45" FontFamily="Yu Gothic" FontWeight="Bold" d:IsHidden="True"/>
