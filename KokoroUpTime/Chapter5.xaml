<Page x:Class="KokoroUpTime.Chapter5"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:KokoroUpTime"
      xmlns:sys="clr-namespace:System;assembly=mscorlib"
      xmlns:gif="https://github.com/XamlAnimatedGif/XamlAnimatedGif"
      xmlns:outline_maker="clr-namespace:OutlineTextMaker"
      mc:Ignorable="d" 
      d:DesignHeight="1080" d:DesignWidth="1920"
      Title="Chapter5"
      Loaded="Window_Loaded">

    <Page.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Animation.xaml"/>
                <ResourceDictionary Source="Styles.xaml"/>
            </ResourceDictionary.MergedDictionaries>
            <Storyboard x:Key="wipe_deep_breath_count5_image">
                <DoubleAnimationUsingKeyFrames Storyboard.TargetName="DeepBreathCount5Image" Storyboard.TargetProperty="(UIElement.OpacityMask).(GradientBrush.GradientStops)[0].(GradientStop.Offset)">
                    <EasingDoubleKeyFrame KeyTime="00:00:00" Value="0.8"/>
                    <EasingDoubleKeyFrame KeyTime="00:00:05" Value="0"/>
                </DoubleAnimationUsingKeyFrames>
                <DoubleAnimationUsingKeyFrames Storyboard.TargetName="DeepBreathCount5Image" Storyboard.TargetProperty="(UIElement.OpacityMask).(GradientBrush.GradientStops)[1].(GradientStop.Offset)">
                    <EasingDoubleKeyFrame KeyTime="00:00:00" Value="1"/>
                    <EasingDoubleKeyFrame KeyTime="00:00:05" Value="0"/>
                </DoubleAnimationUsingKeyFrames>
            </Storyboard>
            <Storyboard x:Key="wipe_deep_breath_count1_image">
                <DoubleAnimationUsingKeyFrames Storyboard.TargetName="DeepBreathCount1Image" Storyboard.TargetProperty="(UIElement.OpacityMask).(GradientBrush.GradientStops)[0].(GradientStop.Offset)">
                    <EasingDoubleKeyFrame KeyTime="00:00:00" Value="0.8"/>
                    <EasingDoubleKeyFrame KeyTime="00:00:01" Value="0"/>
                </DoubleAnimationUsingKeyFrames>
                <DoubleAnimationUsingKeyFrames Storyboard.TargetName="DeepBreathCount1Image" Storyboard.TargetProperty="(UIElement.OpacityMask).(GradientBrush.GradientStops)[1].(GradientStop.Offset)">
                    <EasingDoubleKeyFrame KeyTime="00:00:00" Value="1"/>
                    <EasingDoubleKeyFrame KeyTime="00:00:01" Value="0"/>
                </DoubleAnimationUsingKeyFrames>
            </Storyboard>
            <Storyboard x:Key="wipe_deep_breath_count10_image">
                <DoubleAnimationUsingKeyFrames Storyboard.TargetName="DeepBreathCount10Image" Storyboard.TargetProperty="(UIElement.OpacityMask).(GradientBrush.GradientStops)[0].(GradientStop.Offset)">
                    <EasingDoubleKeyFrame KeyTime="00:00:00" Value="0.8"/>
                    <EasingDoubleKeyFrame KeyTime="00:00:10" Value="0"/>
                </DoubleAnimationUsingKeyFrames>
                <DoubleAnimationUsingKeyFrames Storyboard.TargetName="DeepBreathCount10Image" Storyboard.TargetProperty="(UIElement.OpacityMask).(GradientBrush.GradientStops)[1].(GradientStop.Offset)">
                    <EasingDoubleKeyFrame KeyTime="00:00:00" Value="1"/>
                    <EasingDoubleKeyFrame KeyTime="00:00:10" Value="0"/>
                </DoubleAnimationUsingKeyFrames>
            </Storyboard>
            <Storyboard x:Key="fade_out_main_grid">
                <DoubleAnimationUsingKeyFrames Storyboard.TargetName="MainGrid" Storyboard.TargetProperty="(UIElement.Opacity)">
                    <EasingDoubleKeyFrame KeyTime="00:00:03" Value="0"/>
                </DoubleAnimationUsingKeyFrames>
            </Storyboard>
            <Storyboard x:Key="fade_in_main_grid">
                <DoubleAnimationUsingKeyFrames Storyboard.TargetName="MainGrid" Storyboard.TargetProperty="(UIElement.Opacity)">
                    <EasingDoubleKeyFrame KeyTime="00:00:00" Value="0"/>
                    <EasingDoubleKeyFrame KeyTime="00:00:01" Value="1"/>
                </DoubleAnimationUsingKeyFrames>
            </Storyboard>
            <Style TargetType="{x:Type Button}" x:Key="InputButton">
                <Setter Property="Background" Value="White"/>
                <Setter Property="OverridesDefaultStyle" Value="True"/>
                <Setter Property="Template">
                    <Setter.Value>
                        <ControlTemplate TargetType="{x:Type Button}">
                            <Border Background="{TemplateBinding Background}"
                                BorderBrush="{TemplateBinding BorderBrush}"
                                BorderThickness="{TemplateBinding BorderThickness}"
                                CornerRadius="30" >
                                <ContentPresenter />
                            </Border>
                        </ControlTemplate>
                    </Setter.Value>
                </Setter>
                <Style.Triggers>
                    <Trigger Property="IsMouseOver" Value="True">
                        <Setter Property="Background" Value="#FFBFE6FD"/>
                    </Trigger>
                </Style.Triggers>
            </Style>
            <Style x:Key="TextInput" TargetType="{x:Type ListViewItem}">
                <Setter Property="Margin" Value="0,5"/>
                <Setter Property="Template">
                    <Setter.Value>
                        <ControlTemplate TargetType="{x:Type ContentControl}">
                            <Border Height="170" Width="1560" Background="{x:Null}" BorderBrush="Black" BorderThickness="2">
                                <StackPanel   Orientation="Horizontal" Background="Transparent" MouseDown="StackPanel_MouseDown"  >
                                    <TextBlock Text="{Binding}" FontSize="90" FontFamily="Yu Gothic" VerticalAlignment="Center" />
                                    <TextBlock Style="{StaticResource OutputTextBlock}" FontSize="81" Width="1471" VerticalAlignment="Center" />
                                </StackPanel>
                            </Border>
                        </ControlTemplate>
                    </Setter.Value>
                </Setter>
            </Style>
            <Style TargetType="{x:Type ListViewItem}" x:Key="CanvasInput">
                <Setter Property="Margin" Value="0,5"/>
                <Setter Property="Template">
                    <Setter.Value>
                        <ControlTemplate TargetType="{x:Type ContentControl}">
                            <Border Height="170" Width="1560" HorizontalAlignment="Left"  Background="{x:Null}" BorderBrush="Black" BorderThickness="2">
                                <StackPanel Orientation="Horizontal"  Background="Transparent" MouseDown="StackPanel_MouseDown" ScrollViewer.HorizontalScrollBarVisibility="Disabled">
                                    <TextBlock Text="{Binding}" FontSize="90" FontFamily="Yu Gothic" VerticalAlignment="Center" />
                                    <Viewbox Stretch="UniformToFill" >
                                        <InkCanvas Style="{StaticResource OutputInkCanvas}" StrokeCollected="InkCanvas_StrokeCollected" StrokeErased="InkCanvas_StrokeErased">
                                            <InkCanvas.DefaultDrawingAttributes>
                                                <DrawingAttributes Height="5" Width="5" Color="Black"/>
                                            </InkCanvas.DefaultDrawingAttributes>
                                        </InkCanvas>
                                    </Viewbox>
                                </StackPanel>
                            </Border>
                        </ControlTemplate>
                    </Setter.Value>
                </Setter>
            </Style>
            <Style TargetType="{x:Type Button}" x:Key="BodyImageOfKimiBubbleButton">
                <Setter Property="OverridesDefaultStyle" Value="True"/>
                <Setter Property="Background" Value="Transparent"/>
                <Setter Property="Template">
                    <Setter.Value>
                        <ControlTemplate TargetType="{x:Type ContentControl}">
                            <Border Background="{TemplateBinding Background}"/>
                        </ControlTemplate>
                    </Setter.Value>
                </Setter>
            </Style>
            <Style TargetType="{x:Type Image}">
                <Setter Property="RenderTransformOrigin" Value="0.5,0.5"/>
                <Setter Property="RenderTransform">
                    <Setter.Value>
                            <TransformGroup>
                                <ScaleTransform/>
                                <SkewTransform/>
                                <RotateTransform/>
                                <TranslateTransform/>
                            </TransformGroup>
                    </Setter.Value>
                </Setter>
            </Style>
        </ResourceDictionary>
    </Page.Resources>

