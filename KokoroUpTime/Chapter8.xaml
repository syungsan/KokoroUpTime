<Page x:Class="KokoroUpTime.Chapter8"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:gif="https://github.com/XamlAnimatedGif/XamlAnimatedGif"
      xmlns:local="clr-namespace:KokoroUpTime"
      xmlns:outline_marker ="clr-namespace:OutlineTextMaker"
      mc:Ignorable="d" 
      d:DesignHeight="1080" d:DesignWidth="1920"
      Title="Chapter8" Loaded="Window_Loaded">

    <Page.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Animation.xaml"/>
                <ResourceDictionary Source="Styles.xaml"/>
            </ResourceDictionary.MergedDictionaries>
            <local:TitleDataTemplateSelector x:Key="MyTitleDataTemplateSelector"/>
            <DataTemplate x:Key="ThoughtsAndFeelingTitleTemplate">
                <Border x:Name="ThoughtsAndFeelingTitleBorder" Background="#FF4472C4" BorderBrush="#FF2F528F">
                    <outline_marker:OutlineText x:Name="LargePlateTitleText" Text="{Binding TitleText}" FontSize="100"  Style="{StaticResource Orange}"/>
                </Border>
            </DataTemplate>
            <DataTemplate x:Key="HowToUseItemTitleTemplate">
                <Border x:Name="HowToUseItemTitleBorder" Background="#FF4472C4" BorderBrush="#FF2F528F">
                    <Grid x:Name="HowToUseItemTitle">
                        <outline_marker:OutlineText x:Name="UpperRowTitleText" Text="{Binding UpperRowText}" FontSize="80"  Style="{StaticResource Orange}"/>
                        <TextBlock x:Name="RedTitleText" Text="{Binding RedText}" FontSize="100" Foreground="Red" VerticalAlignment="Bottom" Margin="580,0" TextAlignment="Left" LineHeight="125" FontFamily="MS PGothic"/>
                        <outline_marker:OutlineText  x:Name="LowerRowTitleText" Text="{Binding LowerRowText}" FontSize="80" VerticalAlignment="Bottom"  Margin="500,0" Style="{StaticResource Orange}"/>
                    </Grid>
                </Border>
            </DataTemplate>
            <DataTemplate x:Key="ChallengeTimeTitle">
                <Border x:Name="ChallengeTimeTitleBorder" Background="#FF4472C4" BorderBrush="#FF2F528F" d:IsHidden="True">
                    <TextBlock x:Name="ChallengeTimeTitleText" Text="はひふへほ" FontSize="100"  Style="{StaticResource Purple}"/>
                </Border>
            </DataTemplate>
        </ResourceDictionary>
    </Page.Resources>
    <Viewbox>
        <Grid Height="1080" Width="1920" ScrollViewer.VerticalScrollBarVisibility="Visible">
            <Grid x:Name="BaseGrid">
                <Image x:Name="BackgroundImage" Stretch="Fill" Source="/Images/background_01.png"/>
            </Grid>
            <Grid x:Name="TitleGrid"  Margin="0,50,0,800" RenderTransformOrigin="0.5,0.5" Visibility="Visible">
                <Grid.RenderTransform>
                    <TransformGroup>
                        <ScaleTransform/>
                        <SkewTransform/>
                        <RotateTransform/>
                        <TranslateTransform/>
                    </TransformGroup>
                </Grid.RenderTransform>
                <Border Background="#FF4472C4" BorderBrush="#FF2F528F" BorderThickness="3">
                    <outline_marker:OutlineText Style="{StaticResource Orange}" Text="{Binding Text}" FontSize="100" VerticalAlignment="Center" HorizontalAlignment="Center"/>
                </Border>
                <Border  Background="#FFFFC000" BorderBrush="#FFBC8C00" BorderThickness="3">
                    <TextBlock Style="{StaticResource ShadowText}" Text="チャレンジタイム！" Foreground="#FF9966FF" FontSize="100" TextAlignment="Center" VerticalAlignment="Center"/>
                </Border> 
            </Grid>
            <Grid x:Name="MangaGrid" Visibility="Visible" d:IsHidden="True">
                <Border x:Name="MangaTitleBorder" Margin="86,24,20,0" BorderBrush="Black" BorderThickness="6" Height="98" Width="1752" HorizontalAlignment="Left" VerticalAlignment="Top">
                    <outline_marker:OutlineText x:Name="MangaTitleOutlinetext" Text="第８回　「いろいろな考えをしてみよう」" Style="{StaticResource MangaTitle}" />
                </Border>
                <Image x:Name="MangaImage" HorizontalAlignment="Center" VerticalAlignment="Top" Source="/Images/manga1-1.png" Margin="0,121,0,0" RenderTransformOrigin="0.5,0.5" Height="880" Width="1788">
                    <Image.RenderTransform>
                        <TransformGroup>
                            <ScaleTransform/>
                            <SkewTransform/>
                            <RotateTransform/>
                            <TranslateTransform/>
                        </TransformGroup>
                    </Image.RenderTransform>
                </Image>
                <Button x:Name="MangaPrevBackButton" Background="{x:Null}" BorderBrush="{x:Null}" Margin="0,989,1616,-2" Click="Button_Click">
                    <Image x:Name="MangaPrevBackImage" Source="/Images/prev_back_.png" d:IsHidden="True"/>
                </Button>
            </Grid>
            <Grid x:Name="ItemGrid" Visibility="Visible" d:IsHidden="True">
                <Image x:Name="ItemCenterImage" HorizontalAlignment="Left" Height="512" VerticalAlignment="Top" Width="363" Source="/Images/item05_solo.png" Margin="804,266,0,0" RenderTransformOrigin="0.5,0.5" d:IsHidden="True">
                    <Image.RenderTransform>
                        <TransformGroup>
                            <ScaleTransform/>
                            <SkewTransform/>
                            <RotateTransform/>
                            <TranslateTransform/>
                        </TransformGroup>
                    </Image.RenderTransform>
                </Image>
                <Image x:Name="ItemCenterUpImage" HorizontalAlignment="Center" Height="691" VerticalAlignment="Top" Width="490" Source="/Images/item02_solo.png" Margin="0,55,0,0" RenderTransformOrigin="0.5,0.5" d:IsHidden="True">
                    <Image.RenderTransform>
                        <TransformGroup>
                            <ScaleTransform/>
                            <SkewTransform/>
                            <RotateTransform/>
                            <TranslateTransform/>
                        </TransformGroup>
                    </Image.RenderTransform>
                </Image>
                <Image x:Name="ItemLeftImage" HorizontalAlignment="Left" Height="490" VerticalAlignment="Top" Width="346" Source="/Images/item01_solo.png" Margin="187,380,0,0" d:IsHidden="True">
                    <Image.RenderTransform>
                        <TransformGroup>
                            <ScaleTransform/>
                            <SkewTransform/>
                            <RotateTransform/>
                            <TranslateTransform/>
                        </TransformGroup>
                    </Image.RenderTransform>
                </Image>
                <Image x:Name="ItemLeftLastImage" HorizontalAlignment="Left" Height="490" VerticalAlignment="Top" Width="346" Margin="354,186,0,0" Source="/Images/item01_solo.png" d:IsHidden="True"/>
                <Grid x:Name="ItemNamePlateLeftGrid" Width="610" Height="195" Margin="90,55,1220,830" d:IsHidden="True">
                    <Image x:Name="ItemNamePlateLeftImage" HorizontalAlignment="Left" VerticalAlignment="Top" Source="/Images/item_name.png"/>
                    <TextBlock x:Name="ItemNameLeftTextBlock" HorizontalAlignment="Left" Margin="67,36,0,0" VerticalAlignment="Top" FontSize="65" Width="458" FontWeight="Bold" Height="115" Text="おじゃま虫バスター" TextAlignment="Center"/>
                </Grid>
                <Grid x:Name="ItemNamePlateCenterGrid" Width="606" Height="185" Margin="705,234,609,661" d:IsHidden="True">
                    <Image x:Name="ItemNamePlateCenterImage" HorizontalAlignment="Left" VerticalAlignment="Top" Source="/Images/item_name.png" Visibility="Visible"/>
                    <TextBlock x:Name="ItemNameCenterTextBlock" HorizontalAlignment="Left" VerticalAlignment="Top" FontSize="65" Height="105" Width="447" FontWeight="Bold" Margin="67,32,0,0" Text="おじゃま虫バスター" TextAlignment="Center"/>
                </Grid>
                <Grid x:Name="ItemInfoPlateGrid" Margin="661,463,84,250" d:IsHidden="True">
                    <Image x:Name="ItemInfoPlateImage" HorizontalAlignment="Left" VerticalAlignment="Top" Source="/Images/item_detail_info.png" Visibility="Visible" Stretch="Fill"/>
                    <TextBlock x:Name="ItemInfoTitleTextBlock"  FontSize="45" FontWeight="Bold" Margin="55,24,600,270" Text="おじゃま虫バスターとは・・・" FontFamily="MS PGothic"/>
                    <Grid x:Name="ItemInfoSentenceGrid" Margin="55,118,41,33">
                        <TextBlock x:Name="ItemInfoSentenceTextBlock1" FontSize="50" TextWrapping="Wrap" LineHeight="70" FontFamily="MS PGothic">
                            <Run Text="✔ 「おじゃま虫」を発見したときにつかうのじゃ！"/>
                            <Run Text="&#10;✔「おじゃま虫」」をたいじすることができるぞい！"/>
                        </TextBlock>
                    </Grid>
                </Grid>
                <Grid x:Name="ItemNameBubbleGrid" Width="446" Height="240" Margin="172,140,1302,700" d:IsHidden="True">
                    <Image x:Name="ItemNameBubbleImage" HorizontalAlignment="Left" VerticalAlignment="Top" Source="/Images/item_bubble.png"/>
                    <TextBlock x:Name="ItemNumberTextBlock" HorizontalAlignment="Left" Margin="140,118,0,0" VerticalAlignment="Top" FontSize="48" Foreground="#FFEEEE1E" FontWeight="Bold" Text="No. 8"/>
                </Grid>
                <Grid x:Name="ItemLastInfoGrid" Height="116" Width="490" Margin="715,380,715,584">
                    <Image x:Name="ItemLastInfoImage" Source="/Images/item_last.png"/>
                    <TextBlock x:Name="ItemLastInfoTextBlock" HorizontalAlignment="Left" Margin="12,38,0,0" VerticalAlignment="Top" Height="66"  FontSize="45" FontWeight="Bold" Text="No.8  おじゃま虫バスター"/>
                </Grid>
                <Grid x:Name="ItemReviewGrid">
                    <Image x:Name="ItemLeftBigImage" Source="/Images/Item07_solo.png" VerticalAlignment="Center" HorizontalAlignment="Left" Margin="100,200,100,200"/>
                    <Grid x:Name="ItemReviewPlate" Margin="700,300,480,550">
                        <Image x:Name="ItemReviewPlateImage" Source="/Images/item_review.png"  Stretch="Fill"/>
                        <TextBlock x:Name="ItemReviewText1" Margin="15,15,15,130" Text="#7  「考えライト」" FontSize="60"/>
                        <TextBlock x:Name="ItemReviewText2" Margin="15,110,15,15" Text="自分の考えを見つけられます。" TextWrapping="Wrap" FontSize="60"/>
                    </Grid>
                    <Grid x:Name="ItemReviewBubble" Margin="700,620,190,100">
                        <Image x:Name="Item" Source="/Images/item_check_back_bubble.png"  Stretch="Fill"/>
                        <TextBlock x:Name="Text" FontSize="50" TextWrapping="Wrap"  FontFamily="Yu Gothic"  FontWeight="UltraBold"  Margin="40,34,40,12" LineHeight="100">
                            <Run Text="考えのとくちょうは…"/>
                            <Run Text="&#10;➢同じ場面でも人によって考え方がちがう"/>
                            <Run Text="&#10;➢考えがちがうと、きもちもちがう" />
                        </TextBlock>
                    </Grid>
                </Grid>
            </Grid>
            <Grid x:Name="SessionGrid" d:IsHidden="True">
                <Image x:Name="SessionTitleImage" Source="/Images/challenge_time.png" Margin="226,75,906,917" d:IsHidden="True"/>
                <Grid x:Name="SessionFrameGrid" Width="1010" Height="328" Margin="226,243,684,509" d:IsHidden="True">
                    <Image x:Name="SessionFrameImage" HorizontalAlignment="Left" VerticalAlignment="Top" Source="/Images/white_frame.png" Stretch="Fill" d:IsHidden="True"/>
                    <TextBlock x:Name="SessionSubTitleTextBlock" Text="TextBlock" TextWrapping="Wrap" VerticalAlignment="Top" Height="55" FontSize="48" FontWeight="Bold" Margin="44,32,0,0" HorizontalAlignment="Left" Width="745" d:IsHidden="True"/>
                    <TextBlock x:Name="SessionSentenceTextBlock" HorizontalAlignment="Left" Margin="89,112,0,0" Text="TextBlock" TextWrapping="Wrap" VerticalAlignment="Top" Height="168" Width="865" FontSize="48" d:IsHidden="True"/>
                    <TextBlock x:Name="SessionStarTextBlock" HorizontalAlignment="Left" Margin="35,109,0,0" VerticalAlignment="Top" FontSize="48" RenderTransformOrigin="0.345,0.474" FontWeight="Bold" Text="☆" TextAlignment="Center" d:IsHidden="True"/>
                </Grid>
                <TextBlock x:Name="SessionFrameText" Text="きちんとつたえてみよう"  FontWeight="UltraBold" FontSize="70" Background="Yellow" Width="800" Height="120" Margin="10,10,680,500" Foreground="MediumPurple" FontFamily="Yu Gothic" TextAlignment="Center" d:IsHidden="True" />
            </Grid>
            <Grid x:Name="SummaryGrid" d:IsHidden="True">
                <Grid x:Name="SummaryBoardGrid" Margin="496,203,496,305">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition/>
                    </Grid.ColumnDefinitions>
                    <Grid.RowDefinitions>
                        <RowDefinition/>
                    </Grid.RowDefinitions>
                    <Image x:Name="SummaryBoardImage" Source="/Images/blackboard.png"/>
                    <TextBlock x:Name="SummarySubTitleTextBlock" Margin="118,42,118,446" Text="今日の教室で学んだこと" TextWrapping="Wrap" Foreground="White" Background="{x:Null}" FontSize="68" FontWeight="Bold" TextAlignment="Center"/>
                    <TextBlock x:Name="SummarySentenceTextBlock" Text="✓「おじゃま虫」を発見した・&#10;✓「おじゃま虫」をたいじする練習を&#10;　した。"  TextWrapping="Wrap" Foreground="#FFFFE800" FontSize="58" Margin="69,130,66,190"/>
                    <Image x:Name="SummaryItemImage" Source="/Images/item08_solo.png"  Margin="-120,350,500,30" RenderTransformOrigin="0.5,0.5"/>
                    <TextBlock x:Name="SummaryItemText" Margin="250,400,52,75" Foreground="White" FontFamily="MS PGothic" FontWeight="Bold" d:IsHidden="True">
                        <Run x:Name="SummaryItemTitle" Text="本日の発明品" FontSize="40" />
                        <Run x:Name="SummaryItemName" Text="&#10;No.8  おじゃま虫バスター" FontSize="50"/>
                    </TextBlock>
                </Grid>
                <Image x:Name="SummaryTitleImage" Width="401" Source="/Images/summary_title.png" Margin="760,22,759,907" Height="151"/>
            </Grid>
            <Grid x:Name="EndingGrid" d:IsHidden="True">
                <Grid x:Name="EndingTitleGrid" Width="878" Height="427" Margin="521,202,521,451">
                    <Image x:Name="EndingTitleBackImage" Source="/Images/ending_bubble.png" Stretch="Uniform"/>
                    <TextBlock x:Name="EndingTitleTextBlock" HorizontalAlignment="Center" Margin="0,214,0,0" VerticalAlignment="Top" FontSize="88" FontWeight="Bold" Foreground="Black" Text="しゅうりょう！" TextAlignment="Center"/>
                    <TextBlock x:Name="EndingChapterNoTextBlock" HorizontalAlignment="Center" Margin="0,97,0,0" VerticalAlignment="Top" FontSize="88" FontWeight="Bold" Text="第８回" TextAlignment="Center"/>
                </Grid>
                <Image x:Name="EndingItemImage" HorizontalAlignment="Center" Height="340" VerticalAlignment="Top" Width="346" Source="/Images/item08_solo.png" Margin="-100,570,0,0"/>
                <Image x:Name="KimiEndingImage" HorizontalAlignment="Left" Height="470" Margin="1364,10,0,0" VerticalAlignment="Top" Width="436" Source="/Images/kimi_face_02.png"/>
                <Image x:Name="AosukeEndingImage" HorizontalAlignment="Left" Height="470" Margin="120,10,0,0" VerticalAlignment="Top" Width="436" Source="/Images/aosuke_face_01.png" RenderTransformOrigin="0.5,0.5">
                    <Image.RenderTransform>
                        <TransformGroup>
                            <ScaleTransform/>
                            <SkewTransform/>
                            <RotateTransform Angle="0"/>
                            <TranslateTransform/>
                        </TransformGroup>
                    </Image.RenderTransform>
                </Image>
                <Image x:Name="AkamaruEndingImage" HorizontalAlignment="Left" Height="470" Margin="59,460,0,0" VerticalAlignment="Top" Width="436" Source="/Images/akamaru_face_04.png"/>
            </Grid>
            <Grid x:Name="OjamamushiDiscriptionGrid" Margin="200,120,200,200" RenderTransformOrigin="0.5 0.5" d:IsHidden="True" >
                <Grid.RenderTransform>
                    <TransformGroup>
                        <ScaleTransform/>
                        <SkewTransform/>
                        <RotateTransform/>
                        <TranslateTransform/>
                    </TransformGroup>
                </Grid.RenderTransform>
                <TextBlock x:Name="OjamamushiDiscriptionTitleText" Text="「おじゃま虫」とは？" Style="{StaticResource ShadowText}" FontSize="60"  Foreground="Blue" VerticalAlignment="Center" FontFamily="/Fonts/HGRPP1.TTC#HG創英角ﾎﾟｯﾌﾟ体" Margin="0,0,0,660" RenderTransformOrigin="0.5 0.5">
                    <TextBlock.RenderTransform>
                        <TransformGroup>
                            <ScaleTransform/>
                            <SkewTransform/>
                            <RotateTransform/>
                            <TranslateTransform/>
                        </TransformGroup>
                    </TextBlock.RenderTransform>
                </TextBlock>
                <Grid x:Name="OjamamsuhiDiscriptionPlateGrid" RenderTransformOrigin="0.5 0.5">
                    <Grid.RenderTransform>
                        <TransformGroup>
                            <ScaleTransform/>
                            <SkewTransform/>
                            <RotateTransform/>
                            <TranslateTransform/>
                        </TransformGroup>
                    </Grid.RenderTransform>
                    <Rectangle x:Name="OjamamsuhiDiscriptionPlate" Margin="0,100,0,100" Fill="#B2C0DDC8" Stroke="Blue" StrokeDashArray="8 6" StrokeThickness="2" RadiusX="60" RadiusY="60"/>
                    <TextBlock x:Name="OjamamsuhiDiscriptionText" Margin="70,200"  FontFamily="MS PGothic" FontSize="60" LineHeight="100" TextWrapping="Wrap"> 
                        <Run Text="・心の中に住んでいて、いやなきもちにさせたり、&#10;　いやなきもちをひどくさせる考え方のこと。"/>
                        <Run Text="&#10;・じつは考えライトは、心の中にひそんだおじゃま虫を、&#10;　見つけ出すことができる！"/>
                    </TextBlock>
                    <DockPanel x:Name="OjamamushisImageDockPanel" Margin="800,550,0,0" VerticalAlignment="Bottom" HorizontalAlignment="Left">
                        <Image x:Name="OjamamushiImage1"  Source="Images/ojamamushi_01.png"/>
                        <Image x:Name="OjamamushiImage2"  Source="Images/ojamamushi_02.png"/>
                        <Image x:Name="OjamamushiImage3"  Source="Images/ojamamushi_03.png"/>
                    </DockPanel>
                </Grid>
            </Grid>
            <Grid x:Name="ChallengeTimeGrid" Margin="200,50,200,250">
                <Border x:Name="ChallengeTimeBorder" Background="#B2DFC37A" CornerRadius="60" >
                    <Grid x:Name="ChallengeTimeTemplateGrid">
                        <Image x:Name="ChalengeTimeChildImage" Source="Images/kimi_face_01.png" Margin="80,300,80,50" HorizontalAlignment="Left" />
                        <Grid x:Name="ChildrenThoughtSpeechBubbleGrid" Margin="420,90,200,340">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="200"/>
                                <ColumnDefinition Width="800"/>
                            </Grid.ColumnDefinitions>
                            <Border x:Name="ChildrenSpeechBubbleBorder" Height="250" Width="700" Background="White" CornerRadius="60" BorderBrush="#FFFFC000" BorderThickness="10" HorizontalAlignment="Left" Grid.Column="1">
                                <TextBlock x:Name="ChildrensThoughtsText" Text="うまくいくときもあるし、なにをやってもだめではないかも" FontSize="35" TextWrapping="Wrap" Margin="50,50" LineHeight="60" VerticalAlignment="Center" Grid.Column="1" FontWeight="Bold" FontFamily="Yu Gothic"/>
                            </Border>
                            <Path   x:Name="ChildrenSpeechBubbleTailMask" Stroke="White" StrokeThickness="10" StrokeStartLineCap="Round" StrokeEndLineCap="Round" Margin="0,0,-10,0" Data="M205,180 l0,50"/>
                            <Path   x:Name="ChildrenSpeechBubbletail" Fill="White" Stroke="{Binding ElementName=ChildrenSpeechBubbleBorder,Path=BorderBrush}" StrokeThickness="10" StrokeStartLineCap="Round" StrokeEndLineCap="Round" Margin="0,0,-10,0" Data="M205,180 L60,300 205,230"/>
                        </Grid>
                    </Grid>
                </Border>
                <Image x:Name="OjamamushiAttachedSpeechBubbleImage" Margin="1100,50,100,580" Source="Images/ojamamushi_01.png" VerticalAlignment="Top"/>
                <TextBlock x:Name="ChildrensThoughtsTitleText" Text="キミちゃんの考え" Margin="680,70,300,0" FontSize="60" VerticalAlignment="Top" FontFamily="MS PGothic" FontWeight="Bold"/>
                <TextBlock x:Name="OjamamushisTitleText" Text="キミちゃんの考え" Margin="80,70,1000,0" FontSize="60" VerticalAlignment="Top" FontFamily="MS PGothic" FontWeight="Bold"/>
                <Grid x:Name="FeelingInputGrid" Margin="600,400,100,50">
                    <TextBlock x:Name="KindOfFeelingTitleTextBlock" HorizontalAlignment="Left" Margin="70,30,0,0" Text="きもちの種類" TextWrapping="Wrap" VerticalAlignment="Top" FontSize="55" FontWeight="Bold" TextAlignment="Center"/>
                    <TextBlock x:Name="SizeOfFeelingTitleTextBlock" HorizontalAlignment="Left" Margin="470,30,0,0" Text="きもちの大きさ" TextWrapping="Wrap" VerticalAlignment="Top" FontSize="55" FontWeight="Bold"/>
                    <Button x:Name="KindOfFeelingInputButton" Style="{StaticResource DoublelineFrame}" HorizontalAlignment="Left" Width="334" Height="175" BorderBrush="#FF4472C4" BorderThickness="5" Margin="50,100,3,25" Click="Button_Click">
                        <TextBlock x:Name="KindOfFeelingInputTextBlock" Text="TextBlock" FontSize="68" HorizontalAlignment="Left" TextAlignment="Center" VerticalAlignment="Center" FontWeight="Bold"/>
                    </Button>
                    <Button x:Name="SizeOfFeelingInputButton" HorizontalAlignment="Left" Margin="450,130,0,0" VerticalAlignment="Top" FontSize="140" Background="{x:Null}" BorderBrush="{x:Null}" Click="Button_Click" Height="141" Width="347">
                        <Grid x:Name="SizeOfFeelingInputGrid">
                            <Image x:Name="SizeOfFeelingInputImage" Source="/Images/size_of_feeling_input_button_fimage.png"/>
                            <TextBlock x:Name="SizeOfFeelingInputTextBlock" Text="TextBlock" TextWrapping="Wrap" FontSize="68" TextAlignment="Center" VerticalAlignment="Center" FontWeight="Bold"/>
                        </Grid>
                    </Button>
                </Grid>
                <Image x:Name="AnyCharacterImage" Margin="900,450,-450,-250" Height="300" Width="250"  Source="Images/shiroji_01.png" d:IsHidden="True"/>
                <Grid x:Name="AnyCharacterSpeecBubbleGrid" Margin="760,410,160,60">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="250"/>
                        <RowDefinition/>
                    </Grid.RowDefinitions>
                    <Border x:Name="AnyCharacterSpeechBubbleBorder"  Background="White" CornerRadius="60" BorderBrush="Black" BorderThickness="10"  Grid.Row="0">
                        <TextBlock x:Name="AnyCharacterMessageText" Text="キミちゃん、失敗したらどうしよう" FontSize="30" TextWrapping="Wrap" Margin="50,50" LineHeight="60" VerticalAlignment="Center" Grid.Column="1" FontWeight="Bold" FontFamily="Yu Gothic"/>
                    </Border>
                    <Path   x:Name="AnyCharacterSpeechBubbleTailMask" Stroke="White" StrokeThickness="10" StrokeStartLineCap="Round" StrokeEndLineCap="Round" Margin="0,-10,0,0" Data="M303,0 l170,0" Grid.Row="1"/>
                    <Path   x:Name="AnyCharacterSpeechBubbleTail" Fill="White" Stroke="{Binding ElementName=AnyCharacterSpeechBubbleBorder,Path=BorderBrush}" StrokeThickness="10" StrokeStartLineCap="Round" StrokeEndLineCap="Round" Margin="0,-5,0,0" Data="M300,0 L530,50 475,0" Grid.Row="1"/>
                </Grid>
                <Image x:Name="ChallengeItemImage" Margin="1250,300,100,200" Height="250" Width="250"  Source="Images/item08_solo.png"/>
                <Grid x:Name="ChallengeSpeecBubbleGrid" Margin="550,400,150,-200">
                    <Grid.RowDefinitions>
                        <RowDefinition />
                        <RowDefinition Height="170"/>
                    </Grid.RowDefinitions>
                    <Border x:Name="ChallengeSpeechBubbleBorder"  Background="White" CornerRadius="30" BorderBrush="Black" BorderThickness="10"  Grid.Row="1">
                        <TextBlock x:Name="ChallengeMessageText" Text="キミちゃん、失敗したらどうしよう&#10;sdfsdfsdf" FontSize="30" TextWrapping="Wrap" Margin="40,20"   Grid.Column="1" FontWeight="Bold" FontFamily="Yu Gothic"/>
                    </Border>
                    <Path   x:Name="ChallengeSpeechBubbleTailMask" Stroke="White" StrokeThickness="10" StrokeStartLineCap="Round" StrokeEndLineCap="Round" Margin="0,0,0,-10" Data="M550,100 l100,0" Grid.Row="0" VerticalAlignment="Bottom"/>
                    <Path   x:Name="ChallengeSpeechBubbleTail" Fill="White" Stroke="{Binding ElementName=ChallengeSpeechBubbleBorder,Path=BorderBrush}" StrokeThickness="10" StrokeStartLineCap="Round" StrokeEndLineCap="Round" Margin="0,0,0,-10" Data="M550,100 L740,30 650,100" Grid.Row="0" VerticalAlignment="Bottom"/>
                </Grid>
            </Grid>
            <Grid x:Name="HowToUseItemInformationGrid" Margin="110,100,820,400" d:IsHidden="True">
                <Border x:Name="HowToUseItemBorder" Background="#B2A8D3AE" CornerRadius="60"/>
                <Grid x:Name="HowToUseItem_1Grid">
                    <Image x:Name="HowToUseItem_1OjamamushiImage" Source="Images/ojamamushi_03.png" HorizontalAlignment="Left" Height="350" Width="350"/>
                    <Path x:Name="HowToUseItem_1Bector" Data="M330,250 l100,0 0,-40 80,80 -80,80 0,-40 -100,0 0,-82.5" Fill="#FFFF9999" Stroke="Red" StrokeThickness="3"/>
                    <Border x:Name="OtasukemushiBorder" Margin="520,20,20,20" Background="#FFFF9999" BorderBrush="Red" BorderThickness="3" CornerRadius="60">
                        <Grid x:Name="OtasukemushiGrid">
