<Page x:Class="KokoroUpTime.GamePage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:KokoroUpTime"
      mc:Ignorable="d" 
      d:DesignHeight="1080" d:DesignWidth="1920"
      Title="GamePage">

    <Page.Resources>
        <Storyboard x:Key="appear_main_down_right">
            <DoubleAnimationUsingKeyFrames Storyboard.TargetName="MainCharaDownRight" Storyboard.TargetProperty="(UIElement.RenderTransform).(TransformGroup.Children)[3].(TranslateTransform.Y)">
                <EasingDoubleKeyFrame KeyTime="00:00:00" Value="400">
                    <EasingDoubleKeyFrame.EasingFunction>
                        <BounceEase EasingMode="EaseInOut"/>
                    </EasingDoubleKeyFrame.EasingFunction>
                </EasingDoubleKeyFrame>
                <EasingDoubleKeyFrame KeyTime="00:00:01.5000000" Value="0">
                    <EasingDoubleKeyFrame.EasingFunction>
                        <BounceEase EasingMode="EaseInOut"/>
                    </EasingDoubleKeyFrame.EasingFunction>
                </EasingDoubleKeyFrame>
            </DoubleAnimationUsingKeyFrames>
            <DoubleAnimationUsingKeyFrames Storyboard.TargetName="MainCharaDownRight" Storyboard.TargetProperty="(UIElement.Opacity)">
                <EasingDoubleKeyFrame KeyTime="00:00:00" Value="0">
                    <EasingDoubleKeyFrame.EasingFunction>
                        <CircleEase EasingMode="EaseInOut"/>
                    </EasingDoubleKeyFrame.EasingFunction>
                </EasingDoubleKeyFrame>
                <EasingDoubleKeyFrame KeyTime="00:00:01.5000000" Value="1">
                    <EasingDoubleKeyFrame.EasingFunction>
                        <CircleEase EasingMode="EaseInOut"/>
                    </EasingDoubleKeyFrame.EasingFunction>
                </EasingDoubleKeyFrame>
            </DoubleAnimationUsingKeyFrames>
        </Storyboard>
        <Storyboard x:Key="appear_board">
            <DoubleAnimationUsingKeyFrames Storyboard.TargetName="BoardCharacter" Storyboard.TargetProperty="(UIElement.RenderTransform).(TransformGroup.Children)[3].(TranslateTransform.Y)">
                <EasingDoubleKeyFrame KeyTime="00:00:00" Value="63">
                    <EasingDoubleKeyFrame.EasingFunction>
                        <BackEase EasingMode="EaseInOut"/>
                    </EasingDoubleKeyFrame.EasingFunction>
                </EasingDoubleKeyFrame>
                <EasingDoubleKeyFrame KeyTime="00:00:01" Value="0">
                    <EasingDoubleKeyFrame.EasingFunction>
                        <BackEase EasingMode="EaseInOut"/>
                    </EasingDoubleKeyFrame.EasingFunction>
                </EasingDoubleKeyFrame>
            </DoubleAnimationUsingKeyFrames>
            <DoubleAnimationUsingKeyFrames Storyboard.TargetName="BoardCharacter" Storyboard.TargetProperty="(UIElement.Opacity)">
                <EasingDoubleKeyFrame KeyTime="00:00:00" Value="0">
                    <EasingDoubleKeyFrame.EasingFunction>
                        <CircleEase EasingMode="EaseIn"/>
                    </EasingDoubleKeyFrame.EasingFunction>
                </EasingDoubleKeyFrame>
                <EasingDoubleKeyFrame KeyTime="00:00:01" Value="1">
                    <EasingDoubleKeyFrame.EasingFunction>
                        <CircleEase EasingMode="EaseIn"/>
                    </EasingDoubleKeyFrame.EasingFunction>
                </EasingDoubleKeyFrame>
            </DoubleAnimationUsingKeyFrames>
            <DoubleAnimationUsingKeyFrames Storyboard.TargetName="LongBubble" Storyboard.TargetProperty="(UIElement.RenderTransform).(TransformGroup.Children)[0].(ScaleTransform.ScaleX)">
                <EasingDoubleKeyFrame KeyTime="00:00:01" Value="0.00">
                    <EasingDoubleKeyFrame.EasingFunction>
                        <CircleEase EasingMode="EaseInOut"/>
                    </EasingDoubleKeyFrame.EasingFunction>
                </EasingDoubleKeyFrame>
                <EasingDoubleKeyFrame KeyTime="00:00:01.5000000" Value="1.0">
                    <EasingDoubleKeyFrame.EasingFunction>
                        <CircleEase EasingMode="EaseInOut"/>
                    </EasingDoubleKeyFrame.EasingFunction>
                </EasingDoubleKeyFrame>
            </DoubleAnimationUsingKeyFrames>
            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="LongBubble" Storyboard.TargetProperty="(UIElement.Visibility)">
                <DiscreteObjectKeyFrame KeyTime="00:00:00" Value="{x:Static Visibility.Hidden}"/>
                <DiscreteObjectKeyFrame KeyTime="00:00:01" Value="{x:Static Visibility.Visible}"/>
            </ObjectAnimationUsingKeyFrames>
        </Storyboard>
        <Storyboard x:Key="appear_main_small_left_a">
            <DoubleAnimationUsingKeyFrames Storyboard.TargetName="MainCharaSmallLeftA" Storyboard.TargetProperty="(UIElement.RenderTransform).(TransformGroup.Children)[3].(TranslateTransform.Y)">
                <EasingDoubleKeyFrame KeyTime="00:00:00" Value="365">
                    <EasingDoubleKeyFrame.EasingFunction>
                        <CircleEase EasingMode="EaseOut"/>
                    </EasingDoubleKeyFrame.EasingFunction>
                </EasingDoubleKeyFrame>
                <EasingDoubleKeyFrame KeyTime="00:00:01" Value="0.0">
                    <EasingDoubleKeyFrame.EasingFunction>
                        <CircleEase EasingMode="EaseOut"/>
                    </EasingDoubleKeyFrame.EasingFunction>
                </EasingDoubleKeyFrame>
            </DoubleAnimationUsingKeyFrames>
            <DoubleAnimationUsingKeyFrames Storyboard.TargetName="MainCharaSmallLeftA" Storyboard.TargetProperty="(UIElement.Opacity)">
                <EasingDoubleKeyFrame KeyTime="00:00:00" Value="0">
                    <EasingDoubleKeyFrame.EasingFunction>
                        <CircleEase EasingMode="EaseOut"/>
                    </EasingDoubleKeyFrame.EasingFunction>
                </EasingDoubleKeyFrame>
                <EasingDoubleKeyFrame KeyTime="00:00:01" Value="1">
                    <EasingDoubleKeyFrame.EasingFunction>
                        <CircleEase EasingMode="EaseOut"/>
                    </EasingDoubleKeyFrame.EasingFunction>
                </EasingDoubleKeyFrame>
            </DoubleAnimationUsingKeyFrames>
        </Storyboard>
        <Storyboard x:Key="appear_main_small_left_b">
            <DoubleAnimationUsingKeyFrames Storyboard.TargetName="MainCharaSmallLeftB" Storyboard.TargetProperty="(UIElement.RenderTransform).(TransformGroup.Children)[3].(TranslateTransform.Y)">
                <EasingDoubleKeyFrame KeyTime="00:00:00" Value="365">
                    <EasingDoubleKeyFrame.EasingFunction>
                        <CircleEase EasingMode="EaseOut"/>
                    </EasingDoubleKeyFrame.EasingFunction>
                </EasingDoubleKeyFrame>
                <EasingDoubleKeyFrame KeyTime="00:00:01" Value="0.0">
                    <EasingDoubleKeyFrame.EasingFunction>
                        <CircleEase EasingMode="EaseOut"/>
                    </EasingDoubleKeyFrame.EasingFunction>
                </EasingDoubleKeyFrame>
            </DoubleAnimationUsingKeyFrames>
            <DoubleAnimationUsingKeyFrames Storyboard.TargetName="MainCharaSmallLeftB" Storyboard.TargetProperty="(UIElement.Opacity)">
                <EasingDoubleKeyFrame KeyTime="00:00:00" Value="0">
                    <EasingDoubleKeyFrame.EasingFunction>
                        <CircleEase EasingMode="EaseOut"/>
                    </EasingDoubleKeyFrame.EasingFunction>
                </EasingDoubleKeyFrame>
                <EasingDoubleKeyFrame KeyTime="00:00:01" Value="1">
                    <EasingDoubleKeyFrame.EasingFunction>
                        <CircleEase EasingMode="EaseOut"/>
                    </EasingDoubleKeyFrame.EasingFunction>
                </EasingDoubleKeyFrame>
            </DoubleAnimationUsingKeyFrames>
        </Storyboard>
        <Storyboard x:Key="appear_main_small_left_c">
            <DoubleAnimationUsingKeyFrames Storyboard.TargetName="MainCharaSmallLeftC" Storyboard.TargetProperty="(UIElement.RenderTransform).(TransformGroup.Children)[3].(TranslateTransform.Y)">
                <EasingDoubleKeyFrame KeyTime="00:00:00" Value="365">
                    <EasingDoubleKeyFrame.EasingFunction>
                        <CircleEase EasingMode="EaseOut"/>
                    </EasingDoubleKeyFrame.EasingFunction>
                </EasingDoubleKeyFrame>
                <EasingDoubleKeyFrame KeyTime="00:00:01" Value="0.0">
                    <EasingDoubleKeyFrame.EasingFunction>
                        <CircleEase EasingMode="EaseOut"/>
                    </EasingDoubleKeyFrame.EasingFunction>
                </EasingDoubleKeyFrame>
            </DoubleAnimationUsingKeyFrames>
            <DoubleAnimationUsingKeyFrames Storyboard.TargetName="MainCharaSmallLeftC" Storyboard.TargetProperty="(UIElement.Opacity)">
                <EasingDoubleKeyFrame KeyTime="00:00:00" Value="0">
                    <EasingDoubleKeyFrame.EasingFunction>
                        <CircleEase EasingMode="EaseOut"/>
                    </EasingDoubleKeyFrame.EasingFunction>
                </EasingDoubleKeyFrame>
                <EasingDoubleKeyFrame KeyTime="00:00:01" Value="1">
                    <EasingDoubleKeyFrame.EasingFunction>
                        <CircleEase EasingMode="EaseOut"/>
                    </EasingDoubleKeyFrame.EasingFunction>
                </EasingDoubleKeyFrame>
            </DoubleAnimationUsingKeyFrames>
        </Storyboard>
    </Page.Resources>
    <Grid>
        <Grid x:Name="MainGrid" Visibility="Visible" Grid.RowSpan="4" Grid.ColumnSpan="2">
            <Grid.RowDefinitions>
                <RowDefinition Height="12*"/>
                <RowDefinition Height="35*"/>
                <RowDefinition Height="15*"/>
                <RowDefinition Height="55*"/>
                <RowDefinition Height="27*"/>
                <RowDefinition Height="16*"/>
                <RowDefinition Height="20*"/>
                <RowDefinition Height="12*"/>
                <RowDefinition Height="4*"/>
                <RowDefinition Height="5*"/>
                <RowDefinition Height="15*"/>
            </Grid.RowDefinitions>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="16*"/>
                <ColumnDefinition Width="25*"/>
                <ColumnDefinition Width="15*"/>
                <ColumnDefinition Width="25*"/>
                <ColumnDefinition Width="50*"/>
                <ColumnDefinition Width="10*"/>
                <ColumnDefinition Width="26*"/>
                <ColumnDefinition Width="40*"/>
                <ColumnDefinition Width="38*"/>
                <ColumnDefinition Width="20*"/>
                <ColumnDefinition Width="24*"/>
                <ColumnDefinition Width="24*"/>
                <ColumnDefinition Width="16*"/>
                <ColumnDefinition Width="15*"/>
                <ColumnDefinition Width="27*"/>
                <ColumnDefinition Width="13*"/>
            </Grid.ColumnDefinitions>
            <Image x:Name="BG"  Stretch="Fill" Source="/Images/background_01.png" Grid.RowSpan="11" Grid.ColumnSpan="16"/>
            <Image x:Name="MainCharaDownRight" RenderTransformOrigin="0.5,0.5" Source="/Images/shiroji_01.png" Grid.Row="3" Grid.Column="10" Grid.RowSpan="5" Grid.ColumnSpan="5">
                <Image.RenderTransform>
                    <TransformGroup>
                        <ScaleTransform/>
                        <SkewTransform/>
                        <RotateTransform/>
                        <TranslateTransform/>
                    </TransformGroup>
                </Image.RenderTransform>
            </Image>
            <Image x:Name="MainCharaSmallLeftA" Grid.Column="3" HorizontalAlignment="Left" Height="472" VerticalAlignment="Top" Width="420" Source="/Images/akamaru.png" Grid.RowSpan="3" Grid.Row="2" Margin="0,10,0,0" Grid.ColumnSpan="3" RenderTransformOrigin="0.5,0.5">
                <Image.RenderTransform>
                    <TransformGroup>
                        <ScaleTransform/>
                        <SkewTransform/>
                        <RotateTransform/>
                        <TranslateTransform/>
                    </TransformGroup>
                </Image.RenderTransform>
            </Image>
            <Image x:Name="MainCharaSmallLeftB" Grid.Column="4" HorizontalAlignment="Left" Height="493" VerticalAlignment="Top" Width="424" Grid.ColumnSpan="3" Grid.RowSpan="4" Source="/Images/aosuke.png" Margin="3,150,0,0" RenderTransformOrigin="0.5,0.5" Grid.Row="1">
                <Image.RenderTransform>
                    <TransformGroup>
                        <ScaleTransform/>
                        <SkewTransform/>
                        <RotateTransform/>
                        <TranslateTransform/>
                    </TransformGroup>
                </Image.RenderTransform>
            </Image>
            <Image x:Name="MainCharaSmallLeftC" Grid.Column="5" HorizontalAlignment="Left" Height="479" Grid.Row="2" VerticalAlignment="Center" Width="375" Source="/Images/kimil.png" Grid.ColumnSpan="3" Grid.RowSpan="3" RenderTransformOrigin="0.5,0.5">
                <Image.RenderTransform>
                    <TransformGroup>
                        <ScaleTransform/>
                        <SkewTransform/>
                        <RotateTransform/>
                        <TranslateTransform/>
                    </TransformGroup>
                </Image.RenderTransform>
            </Image>
            <Image x:Name="MainInfoCenter" Grid.Column="2" HorizontalAlignment="Left" Height="652" Margin="6,8,0,0" VerticalAlignment="Top" Width="1006" Source="/Images/SS.png" Grid.ColumnSpan="7" Grid.RowSpan="4" Grid.Row="1"/>
            <Image x:Name="MainCharaUpRight" Grid.Column="10" HorizontalAlignment="Left" Height="631" Margin="2,0,0,0" Grid.RowSpan="4" Grid.Row="1" VerticalAlignment="Top" Width="517" Source="/Images/shiroji_03.png" Grid.ColumnSpan="5"/>
            <Image x:Name="BigBubble" Source="/Images/right_big_bubble.png" Grid.Row="4" Grid.ColumnSpan="12" Grid.RowSpan="6" Grid.Column="2" RenderTransformOrigin="0.5,0.5">
                <Image.RenderTransform>
                    <TransformGroup>
                        <ScaleTransform/>
                        <SkewTransform/>
                        <RotateTransform/>
                        <TranslateTransform/>
                    </TransformGroup>
                </Image.RenderTransform>
            </Image>
            <TextBlock x:Name="BigSpeech" Text="TextBlock" TextWrapping="Wrap" FontSize="48" Grid.Row="5" Grid.ColumnSpan="10" Grid.Column="3" Grid.RowSpan="4"/>
            <Button x:Name="NextMessageButton" Background="{x:Null}" BorderBrush="{x:Null}" Foreground="{x:Null}" Click="NextMessageButton_Click" Grid.Row="7" Grid.Column="11" Grid.RowSpan="2" RenderTransformOrigin="0.5,0.5" Grid.ColumnSpan="2">
                <Button.RenderTransform>
                    <TransformGroup>
                        <ScaleTransform/>
                        <SkewTransform/>
                        <RotateTransform/>
                        <TranslateTransform/>
                    </TransformGroup>
                </Button.RenderTransform>
                <Image Source="Images/yazirusi.png"/>
            </Button>
            <Button x:Name="BackMessageButton" Background="{x:Null}" BorderBrush="{x:Null}" Foreground="{x:Null}" Click="BackMessageButton_Click" Grid.Row="5" Grid.Column="11" RenderTransformOrigin="0.5,0.5" Grid.ColumnSpan="2">
                <Button.RenderTransform>
                    <TransformGroup>
                        <ScaleTransform/>
                        <SkewTransform/>
                        <RotateTransform/>
                        <TranslateTransform/>
                    </TransformGroup>
                </Button.RenderTransform>
                <Image Source="Images/yazirusi.png" RenderTransformOrigin="0.5,0.5">
                    <Image.RenderTransform>
                        <TransformGroup>
                            <ScaleTransform/>
                            <SkewTransform/>
                            <RotateTransform Angle="180.0"/>
                            <TranslateTransform/>
                        </TransformGroup>
                    </Image.RenderTransform>
                </Image>
            </Button>

            <Button x:Name="MainNextPageButton" Grid.ColumnSpan="3" Grid.Column="12" Margin="10,0,13,0" Grid.Row="7" VerticalAlignment="Top" Height="180" Grid.RowSpan="4" Background="{x:Null}" BorderBrush="{x:Null}" Click="MainNextPageButton_Click">
                <Image Source="/Images/after.png"/>
            </Button>
            <Button x:Name="MainBackPageButton" HorizontalAlignment="Left" Margin="22,3,0,0" Grid.Row="7" VerticalAlignment="Top" Height="144" Grid.RowSpan="4" Width="278" Grid.ColumnSpan="3" Background="{x:Null}" BorderBrush="{x:Null}" Grid.Column="1">
                <Image Source="/Images/before.png"/>
            </Button>
        </Grid>
        <Grid x:Name="BoardGrid" Grid.ColumnSpan="2" Grid.RowSpan="4" Visibility="Hidden">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="15*"/>
                <ColumnDefinition Width="28*"/>
                <ColumnDefinition Width="30*"/>
                <ColumnDefinition Width="197*"/>
                <ColumnDefinition Width="41*"/>
                <ColumnDefinition Width="30*"/>
                <ColumnDefinition Width="28*"/>
                <ColumnDefinition Width="15*"/>
            </Grid.ColumnDefinitions>
            <Grid.RowDefinitions>
                <RowDefinition Height="37*"/>
                <RowDefinition Height="463*"/>
                <RowDefinition Height="345*"/>
                <RowDefinition Height="112*"/>
                <RowDefinition Height="63*"/>
                <RowDefinition Height="60*"/>
            </Grid.RowDefinitions>
            <Image Source="/Images/background_03.png" Stretch="Fill" Grid.RowSpan="6" Grid.ColumnSpan="8"/>
            <Button x:Name="BordButton" Click="BoardButton_Click" Grid.Column="2" Background="{x:Null}" BorderBrush="{x:Null}" Grid.Row="1" Grid.ColumnSpan="4" Grid.RowSpan="3">
                <Grid>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="54*"/>
                        <ColumnDefinition Width="41*"/>
                        <ColumnDefinition Width="107*"/>
                        <ColumnDefinition Width="38*"/>
                        <ColumnDefinition Width="46*"/>
                    </Grid.ColumnDefinitions>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="77*"/>
                        <RowDefinition Height="145*"/>
                        <RowDefinition Height="175*"/>
                        <RowDefinition Height="175*"/>
                        <RowDefinition Height="175*"/>
                        <RowDefinition Height="130*"/>
                    </Grid.RowDefinitions>
                    <Image Source="/Images/blackboard.png" Grid.RowSpan="6" Grid.ColumnSpan="5" />
                    <TextBlock x:Name="BoardTitle" Text="教室のルール" Foreground="White" FontSize="72" Grid.Column="2" Margin="0,10,0,0" Grid.Row="1"/>
                    <TextBlock x:Name="BoardChecK1Msg" Text="１．人のことを わらわない" Foreground="#FFFFE64A" FontSize="64" Grid.Row="2" Grid.ColumnSpan="2" Grid.Column="2" Margin="0,18,0,5"/>
                    <TextBlock x:Name="BoardChecK2Msg" Text="２．ふざけない" Foreground="#FFFFE64A" FontSize="64" Grid.Row="3" Grid.ColumnSpan="2" Grid.Column="2" Margin="0,21,0,10"/>
                    <TextBlock x:Name="BoardChecK3Msg" Text="３．はずかしがらない" Foreground="#FFFFE64A" FontSize="64" Grid.Row="4" Grid.ColumnSpan="2" Grid.Column="2" Margin="0,21,0,10"/>
                    <CheckBox x:Name="BoardCheck1Box" FontSize="72" Foreground="#FFFFE64A" BorderBrush="#FFFFE64A" Background="#FF003300" Checked="BoardCheck1Box_Checked" Grid.Row="2" Grid.Column="1" Margin="0,10,180,145">
                        <CheckBox.RenderTransform>
                            <ScaleTransform ScaleX="8.0" ScaleY="8.0"/>
                        </CheckBox.RenderTransform>
                    </CheckBox>
                    <CheckBox x:Name="BoardCheck2Box" FontSize="72" Foreground="#FFFFE64A" BorderBrush="#FFFFE64A" Background="#FF003300" Checked="BoardCheck2Box_Checked" Grid.Row="3" Grid.Column="1" Margin="0,10,180,145">
                        <CheckBox.RenderTransform>
                            <ScaleTransform ScaleX="8.0" ScaleY="8.0"/>
                        </CheckBox.RenderTransform>
                    </CheckBox>
                    <CheckBox x:Name="BoardCheck3Box" FontSize="72" Foreground="#FFFFE64A" BorderBrush="#FFFFE64A" Background="#FF003300" Checked="BoardCheck3Box_Checked" Grid.Row="4" Grid.Column="1" Margin="0,0,180,153">
                        <CheckBox.RenderTransform>
                            <ScaleTransform ScaleX="8.0" ScaleY="8.0"/>
                        </CheckBox.RenderTransform>
                    </CheckBox>
                </Grid>
            </Button>
            <Image x:Name="BoardCharacter" Source="/Images/shiroji_02.png" RenderTransformOrigin="0.5,0.5" Grid.Row="2" Grid.RowSpan="4" Grid.Column="4" Grid.ColumnSpan="3">
                <Image.RenderTransform>
                    <TransformGroup>
                        <ScaleTransform/>
                        <SkewTransform/>
                        <RotateTransform/>
                        <TranslateTransform/>
                    </TransformGroup>
                </Image.RenderTransform>
            </Image>
            <Button x:Name="LongBubble" Background="{x:Null}" BorderBrush="{x:Null}" RenderTransformOrigin="0.5,0.5" Click="LongBubble_Click" Grid.Row="3" Grid.RowSpan="2" Grid.Column="2" Grid.ColumnSpan="4">
                <Button.RenderTransform>
                    <TransformGroup>
                        <ScaleTransform/>
                        <SkewTransform/>
                        <RotateTransform/>
                        <TranslateTransform/>
                    </TransformGroup>
                </Button.RenderTransform>
                <Grid>
                    <Image Source="/Images/long_bubble.png"/>
                    <TextBlock x:Name="LongSpeech" Text="以上のルールは必ず守るのじゃ！ルールがわかったら□をタップしてくれるかの？" FontSize="42" Margin="31,55,37,10"/>
                </Grid>
            </Button>

            <Button x:Name="BoardNextPageButton" Background="{x:Null}" BorderBrush="{x:Null}" Foreground="{x:Null}" Click="BoardNextPageButton_Click" Grid.Row="4" Grid.RowSpan="2" Grid.Column="5" Grid.ColumnSpan="2">
                <Image Source="Images/after.png"/>
            </Button>
            <Button x:Name="BoardBackPageButton" Background="{x:Null}" BorderBrush="{x:Null}" Foreground="{x:Null}" Grid.Row="4" Grid.RowSpan="2" Grid.ColumnSpan="2" Grid.Column="1">
                <Image Source="Images/before.png"/>
            </Button>
        </Grid>
    </Grid>
</Page>
